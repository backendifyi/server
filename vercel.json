{
    "builds": [{
        "src": "server/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.9" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build"
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/wsgi.py"
        }
    ]
}